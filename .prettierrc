{
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 250,
  "endOfLine": "auto",
  "semi": false,
  "vueIndentScriptAndStyle": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "trailingComma": "all",
  "jsxSingleQuote": false,
  "arrowParens": "avoid",
  "insertPragma": false,
  "requirePragma": false,
  "proseWrap": "never",
  "htmlWhitespaceSensitivity": "strict",
  "braceSpacing": true,
  "spaceBeforeFunctionParen": true,
  "spaceInParens": true
}